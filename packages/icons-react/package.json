{
  "name": "@pf-ui/pf-icons-react",
  "version": "1.1.42",
  "private": false,
  "description": "支持react的icon库",
  "author": "xuzhudong <451547014@qq.com>",
  "homepage": "",
  "license": "ISC",
  "main": "lib/index.js",
  "module": "es/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "cleanup": "cross-env ./scripts/cleanup.sh",
    "clean": "rimraf es && rimraf lib && npm run cleanup",
    "dev": "vite",
    "prepublishOnly": "npm run generate && npm run build",
    "generate": "rm -rf src/icons && cross-env TS_NODE_PROJECT=scripts/tsconfig.json node -r ts-node/register scripts/generate.ts --target=icon",
    "postbuild": "npm run cleanup && cross-env TS_NODE_PROJECT=scripts/tsconfig.json node -r ts-node/register scripts/generate.ts --target=entry",
    "build": "vite build"
  },
  "peerDependencies": {
    "react": ">=16.0.0",
    "react-dom": ">=16.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.136",
    "@types/node": "^18.8.5",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@umijs/fabric": "^2.2.2",
    "@vitejs/plugin-react": "^2.1.0",
    "cross-env": "^5.2.0",
    "glob": "^7.1.6",
    "lodash": "^4.17.15",
    "prettier": "^2.2.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "rimraf": "^3.0.0",
    "styled-components": "^3.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.1",
    "vite-plugin-dts": "^1.5.0"
  },
  "dependencies": {
    "@pf-ui/pf-icon-svg": "^1.1.42",
    "classnames": "^2.2.6",
    "rc-util": "^5.9.4"
  },
  "repository": {
    "type": "git",
    "url": "http://gitlab.wowjoy.com/platform/front-npm/pf-ui-icon.git"
  },
  "publishConfig": {
    "access": "public"
  },
